ue3/v3_echarts/v3_echarts/myapp/src/components/itemThree.vue","sourceRoot":"","sourcesContent":["<template>\n  <div>\n    <h2>库存统计</h2>\n    <div class=\"chart\" id=\"myEcharts\">图表的容器</div>\n  </div>\n</template>\n\n<script>\nimport { inject, onMounted, reactive } from \"vue\";\nexport default {\n  setup() {\n    let $echarts = inject(\"echarts\");\n    let $http = inject(\"axios\");\n\n    let data = reactive({});\n\n    async function getState() {\n      data = await $http({ url: \"/three/data\" });\n    }\n\n    onMounted(() => {\n      getState().then(() => {\n        console.log(\"饼状图\", data);\n\n        let myChart = $echarts.init(document.getElementById(\"myEcharts\"));\n        myChart.setOption({\n          legend: {\n            top: \"bottom\",\n          },\n          tooltip: {\n            show: true,\n          },\n          series: [\n            {\n              type: \"pie\",\n              data: data.data.chartData.chartData,\n              radius: [10, 100],\n              center: [\"50%\", \"45%\"],\n              roseType: \"area\",\n              itemStyle: {\n                borderRadius: 10,\n              },\n            },\n          ],\n        });\n      });\n    });\n    return {\n      getState,\n      data,\n    };\n  },\n};\n</script>\n\n<style>\nh2 {\n  height: 0.6rem;\n  color: #fff;\n  line-height: 0.6rem;\n  font-size: 0.25rem;\n  text-align: center;\n}\n.chart {\n  height: 4.5rem;\n}\n</style>\n"]}]}